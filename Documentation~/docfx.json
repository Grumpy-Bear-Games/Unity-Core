{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Runtime/**.cs",
            "Editor/**.cs"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Games.GrumpyBear.Core",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath" : "images/favicon.ico",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "content": [
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      },
      {
        "src": "api",
        "files": [
          "**.yml"
        ],
        "dest": "api"
      },
      {
        "src": "manual",
        "files": [
          "**.md",
          "toc.yml"
        ],
        "dest": "manual"
      },
      {
        "src": "..",
        "files": [
          "CHANGELOG.md"
        ],
        "dest": "changelog"
      },
      {
        "src": "..",
        "files": [
          "LICENSE.md"
        ],
        "dest": "license"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
      {
        "files": [
          "favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "sitemap":{
      "baseUrl": "https://grumpy-bear-games.github.io/Unity-Core/",
      "priority": 0.1,
      "changefreq": "weekly"
    },
    "dest": "../_site~",
    "intermediateFolder": "_docfx_cache~",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "fileMetadata": {
      "assemblies": {
        "api/Games.GrumpyBear.Core.*.yml": ["Games.GrumpyBear.Core"],
        "api/Games.GrumpyBear.Core.Editor.*.yml": ["Games.GrumpyBear.Core.Editor"],
        "api/Games.GrumpyBear.Core.InputSystem.*.yml": ["Games.GrumpyBear.Core.InputSystem"]
      }
    },
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "xref": [ "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml" ],
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
